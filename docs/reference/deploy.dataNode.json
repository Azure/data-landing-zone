        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "mySql001Deployment",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/resourceGroups', variables('metadataResourceGroupName'))]",
                "keyVault002Deployment"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('mySqlTemplateLink')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[variables('location')]"
                    },
                    "mySqlServerName": {
                        "value": "[take(concat(variables('enterpriseScaleAnalyticsCompanyPrefix'), '-', variables('dataNodeName'), '-mysqlserver001', uniqueString(subscription().subscriptionId, resourceGroup().id)), 63)]"
                    },
                    "mySqlDatabaseName": {
                        "value": "HiveMetastoreDB"
                    },
                    "mySqlSkuName": {
                        "value": "B_Gen5_1"
                    },
                    "mySqlSkuTier": {
                        "value": "Basic"
                    },
                    "mySqlSkuCapacity": {
                        "value": 1
                    },
                    "mySqlSkuSize": {
                        "value": 5120
                    },
