{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "storageAccountName": {
      "value": "mydatalandingzonenameworkspacesa"
    },
    "storageAccountType": {
      "value": "Standard_ZRS"
    },
    "enableHns": {
      "value": true
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/mydatalandingzonename-network/providers/Microsoft.Network/virtualNetworks/mydatalandingzonename-vnet/subnets/mydatalandingzonename-privatelink-subnet"
    },
    "privateDnsZoneIdBlob": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.blob.core.windows.net"
    },
    "privateDnsZoneIdDfs": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.dfs.core.windows.net"
    }
  }
}
