{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "storageAccountName": {
      "value": "<my-data-landing-zone-name>secretenrcursa"
    },
    "storageAccountType": {
      "value": "Standard_ZRS"
    },
    "enableHns": {
      "value": true
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-network/providers/Microsoft.Network/virtualNetworks/<my-data-landing-zone-name>-vnet/subnets/<my-data-landing-zone-name>-privatelink-subnet"
    },
    "privateDnsZoneIdBlob": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.blob.core.windows.net"
    },
    "privateDnsZoneIdDfs": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.dfs.core.windows.net"
    }
  }
}
