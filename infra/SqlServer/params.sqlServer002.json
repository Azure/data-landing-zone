{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "sqlServerName": {
      "value": "mydatalandingzonename-sqlserver002"
    },
    "administratorLogin": {
      "value": "SqlServerMainUser"
    },
    "administratorLoginPassword": {
      "value": "<your-secure-password>"
    },
    "sqlAdminGroupName": {
      "value": ""
    },
    "sqlAdminGroupObjectID": {
      "value": ""
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/mydatalandingzonename-network/providers/Microsoft.Network/virtualNetworks/mydatalandingzonename-vnet/subnets/mydatalandingzonename-privatelink-subnet"
    },
    "privateDnsZoneId": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.database.windows.net"
    }
  }
}
