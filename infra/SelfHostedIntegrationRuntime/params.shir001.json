{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "vmssName": {
      "value": "<my-data-landing-zone-name>Shir001"
    },
    "vmssSkuTier": {
      "value": "Standard"
    },
    "vmssSkuName": {
      "value": "Standard_DS2_v2"
    },
    "vmssSkuCapacity": {
      "value": 1
    },
    "vmssAdminUsername": {
      "value": "VmssMainUser"
    },
    "vmssAdminPassword": {
      "value": "<your-secure-password>"
    },
    "dataFactoryId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-integration/providers/Microsoft.DataFactory/factories/<my-data-landing-zone-name>-datafactory002"
    },
    "storageAccountId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-integration/providers/Microsoft.Storage/storageAccounts/<my-data-landing-zone-name>artifactstorage001"
    },
    "storageAccountContainerName": {
      "value": "scripts"
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-network/providers/Microsoft.Network/virtualNetworks/<my-data-landing-zone-name>-vnet/subnets/<my-data-landing-zone-name>-subnet"
    }
  }
}
