{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "synapseWorkspaceName": {
      "value": "mydatalandingzonename-synapse001"
    },
    "synapseDefaultStorageAccountName": {
      "value": "mydatalandingzonenameworkspacesa"
    },
    "synapseDefaultStorageAccountFileSystem": {
      "value": "data"
    },
    "synapseSqlAdministratorLogin": {
      "value": "SqlServerMainUser"
    },
    "synapseSqlAdministratorPassword": {
      "value": "<your-secure-password>"
    },
    "synapseDatalakeAnalyticsResourceId": {
      "value": ""
    },
    "synapseComputeSubnetResourceId": {
      "value": ""
    },
    "synapseSqlAdminGroupName": {
      "value": ""
    },
    "synapseSqlAdminGroupObjectID": {
      "value": ""
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/mydatalandingzonename-network/providers/Microsoft.Network/virtualNetworks/mydatalandingzonename-vnet/subnets/mydatalandingzonename-privatelink-subnet"
    },
    "privateDnsZoneIdSql": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.sql.azuresynapse.net"
    },
    "privateDnsZoneIdDev": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.dev.azuresynapse.net"
    }
  }
}
